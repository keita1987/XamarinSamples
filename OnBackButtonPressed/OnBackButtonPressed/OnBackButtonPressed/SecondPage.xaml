<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:onBackButtonPressed="clr-namespace:OnBackButtonPressed;assembly=OnBackButtonPressed"
             x:Class="OnBackButtonPressed.SecondPage"
             Title="Second Page">
  <ContentPage.BindingContext>
    <onBackButtonPressed:SecondPageViewModel/>
  </ContentPage.BindingContext>
  <StackLayout VerticalOptions="Center" HorizontalOptions="Center">
    <Label Text="Can GoBack"></Label>
    <Switch IsToggled="{Binding IsEnabled}"/>
  </StackLayout>
</ContentPage>